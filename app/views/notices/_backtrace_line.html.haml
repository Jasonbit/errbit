%tr{:class => defined?(row_class) ? row_class : nil}
  %td.line{:class => (Notice.in_app_backtrace_line?(line) ? 'in-app' : nil)}
    %span.path>= path_for_backtrace_line(line)
    %span.file= link_to_source_file(@app, line, file_for_backtrace_line(line)).html_safe
    &rarr;
    %span.method= line['method']
